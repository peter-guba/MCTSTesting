<?xml version="1.0"?>
<Behavior Name="FindCarrierAndDock" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Sequence>
    <Behaviors>
      
      <Inverter>
        <UnitHasTarget />
      </Inverter>
      <FindClosestCarrier />
      <Inverter>
        <UnitIsMoving />
      </Inverter>
      <FindPathToTarget PathTargetPosition="true" />
      <UnitMoveAction />
      
    </Behaviors>
  </Sequence>
</Behavior>